{
  "name": "mrc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jss": "^8.1.0",
    "jss-preset-default": "^3.0.0",
    "material-ui": "^1.0.0-beta.5",
    "material-ui-icons": "^1.0.0-alpha.19",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.0.11",
    "react-select": "^1.0.0-rc.5",
    "react-snapshot": "^2.0.0-1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-persist": "^4.8.3",
    "redux-saga": "^0.15.6",
    "seamless-immutable": "^7.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run snapshot",
    "prettier": "prettier --single-quote --print-width 120 --trailing-comma all \"src/**/*.js\"",
    "prettier:ci": "npm run prettier -- --list-different",
    "prettier:fix": "npm run prettier -- --write",
    "snapshot": "react-snapshot",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "prettier": "^1.5.3"
  },
  "reactSnapshot": {
    "include": [
      "/",
      "/application",
      "/application/loan",
      "/application/personal",
      "/application/financial"
    ],
    "exclude": [
      "/application/summary"
    ],
    "snapshotDelay": 300
  }
}
